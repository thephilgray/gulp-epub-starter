{
  "name": "gulp-epub-starter",
  "version": "1.1.0",
  "description": "An opinionated static EPUB generator with Less and BrowserSync + Readium Cloud Reader for live reload development.",
  "scripts": {
    "dev": "NODE_ENV=development gulp --require @babel/register --gulpfile tasks",
    "dev:ipad": "DEVICE=ipad NODE_ENV=development gulp --require @babel/register --gulpfile tasks",
    "dev:ipad:fixed": "DEVICE=ipad NODE_ENV=development gulp --require @babel/register --gulpfile tasks --fixed",
    "dev:android": "DEVICE=android NODE_ENV=development gulp --require @babel/register --gulpfile tasks",
    "dev:android:fixed": "DEVICE=android NODE_ENV=development gulp --require @babel/register --gulpfile tasks --fixed",
    "dev:kindle": "DEVICE=kindle NODE_ENV=development gulp --require @babel/register --gulpfile tasks",
    "dev:kindle:fixed": "DEVICE=kindle NODE_ENV=development gulp --require @babel/register --gulpfile tasks --fixed",
    "build": "NODE_ENV=production gulp build --require @babel/register --gulpfile tasks",
    "build:ipad": "DEVICE=ipad NODE_ENV=production gulp build --require @babel/register --gulpfile tasks",
    "build:ipad:fixed": "DEVICE=ipad NODE_ENV=production gulp build --require @babel/register --gulpfile tasks --fixed",
    "build:android": "DEVICE=android NODE_ENV=production gulp build --require @babel/register --gulpfile tasks",
    "build:android:fixed": "DEVICE=android NODE_ENV=production gulp build --require @babel/register --gulpfile tasks --fixed",
    "build:kindle": "DEVICE=kindle NODE_ENV=production gulp build --require @babel/register --gulpfile tasks",
    "build:kindle:fixed": "DEVICE=kindle NODE_ENV=production gulp build --require @babel/register --gulpfile tasks --fixed",
    "publish": "DEVICE=ipad NODE_ENV=production gulp publish --require @babel/register --gulpfile tasks",
    "publish:ipad": "DEVICE=ipad NODE_ENV=production gulp publish --require @babel/register --gulpfile tasks",
    "publish:ipad:fixed": "DEVICE=ipad NODE_ENV=production gulp publish --require @babel/register --gulpfile tasks --fixed",
    "publish:android": "DEVICE=android NODE_ENV=production gulp publish --require @babel/register --gulpfile tasks",
    "publish:android:fixed": "DEVICE=android NODE_ENV=production gulp publish --require @babel/register --gulpfile tasks --fixed",
    "publish:kindle": "DEVICE=kindle NODE_ENV=production gulp publishKindle --require @babel/register --gulpfile tasks",
    "publish:kindle:fixed": "DEVICE=kindle NODE_ENV=production gulp publishKindle --require @babel/register --gulpfile tasks --fixed",
    "validate": "gulp validate --require @babel/register --gulpfile tasks",
    "eslint-check": "eslint --print-config . | eslint-config-prettier-check"
  },
  "keywords": [
    "epub",
    "gulp",
    "ebook",
    "ebook starter",
    "live-reload"
  ],
  "author": "@thephilgray",
  "license": "ISC",
  "devDependencies": {
    "@babel/core": "^7.0.0-beta.54",
    "@babel/preset-env": "^7.0.0-beta.54",
    "@babel/register": "^7.0.0-beta.54",
    "autoprefixer": "^9.1.0",
    "css-loader": "^1.0.0",
    "date-fns": "^1.29.0",
    "del": "^3.0.0",
    "epub-zip": "0.0.1",
    "eslint": "^5.3.0",
    "eslint-config-prettier": "^3.0.1",
    "eslint-config-standard": "^11.0.0",
    "eslint-plugin-import": "^2.14.0",
    "eslint-plugin-node": "^7.0.1",
    "eslint-plugin-prettier": "^2.6.2",
    "eslint-plugin-promise": "^3.8.0",
    "eslint-plugin-standard": "^3.1.0",
    "fs-extra": "^7.0.1",
    "gulp": "^4.0.0",
    "gulp-add-less-variables": "^1.0.4",
    "gulp-cli": "^2.0.1",
    "gulp-concat": "^2.6.1",
    "gulp-concat-css": "^3.1.0",
    "gulp-cssbeautify": "^1.0.0",
    "gulp-data": "^1.3.1",
    "gulp-ext-replace": "^0.3.0",
    "gulp-file-assets": "^3.0.0",
    "gulp-filelist": "^1.0.2",
    "gulp-filter": "^5.1.0",
    "gulp-htmltidy": "^0.2.4",
    "gulp-if": "^2.0.2",
    "gulp-ignore": "^2.0.2",
    "gulp-image": "^4.3.0",
    "gulp-less": "^4.0.1",
    "gulp-merge": "^0.1.1",
    "gulp-postcss": "^7.0.1",
    "gulp-pug": "^4.0.1",
    "gulp-purgecss": "^1.1.1",
    "gulp-rename": "^1.4.0",
    "gulp-sourcemaps": "^2.6.4",
    "js-yaml": "^3.12.1",
    "launch-epub": "git+https://github.com/thephilgray/launch-epub.git",
    "lodash": "^4.17.10",
    "merge-stream": "^1.0.1",
    "minimist": "^1.2.0",
    "mkdirp": "^0.5.1",
    "prettier": "^1.14.2",
    "regenerator-runtime": "^0.12.1",
    "vinyl-buffer": "^1.0.1",
    "vinyl-source-stream": "^2.0.0"
  },
  "dependencies": {},
  "babel": {
    "presets": [
      [
        "@babel/preset-env",
        {
          "useBuiltIns": "usage",
          "debug": false
        }
      ]
    ]
  }
}
